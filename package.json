{
  "name": "comento-ui",
  "version": "1.1.10",
  "license": "MIT",
  "main": "./dist/comento-ui.umd.js",
  "module": "./dist/comento-ui.es.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/comento-ui.es.js",
      "require": "./dist/comento-ui.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/comento/comento-ui.git"
  },
  "homepage": "https://github.com/comento/comento-ui",
  "scripts": {
    "vite-dev": "vite",
    "vite-build": "vite build && yarn copy",
    "vite-lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "serve": "vite preview",
    "bundle": "vue-cli-service build --target lib --name comento-ui src/main.js",
    "copy": "yarn copy-color && yarn copy-fonts && yarn copy-mixins",
    "copy-color": "cp -R src/assets/style/base/color.scss dist && cp -R src/utils/constants/color.js dist/color.js",
    "copy-fonts": "cp -R src/assets/style/base/fonts.scss dist",
    "copy-mixins": "cp -R src/assets/style/base/mixins.scss dist && cp -R src/assets/style/base/flexbox.scss dist && cp -R src/assets/style/base/functions.scss dist",
    "web-types": "vue-docgen-web-types",
    "build": "yarn bundle && yarn copy && yarn web-types && yarn make-prop-tags",
    "lint": "vue-cli-service lint",
    "release": "shipjs prepare",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic --project-token",
    "deploy-storybook": "storybook-to-ghpages",
    "test": "jest",
    "test:watchAll": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "make-prop-tags": "vue-int --input src/components --output dist --recursive"
  },
  "dependencies": {
    "@storybook/storybook-deployer": "^2.8.7",
    "@vitejs/plugin-vue": "^2.3.3",
    "swiper": "5.x",
    "v-tooltip": "^2.0.3",
    "vite": "^3.2.5",
    "vite-plugin-mdx": "^3.5.11",
    "vite-plugin-vue2": "^1.9.0",
    "vue": "2.7",
    "vue-awesome-swiper": "^4.1.1",
    "vue-linkify": "^1.0.1",
    "vue-pagination-2": "^3.0.91",
    "vue-template-compiler": "^2.6.11",
    "vue2-datepicker": "^3.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-docs": "^6.0.22",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-viewport": "^6.0.26",
    "@storybook/builder-vite": "^0.4.0",
    "@storybook/mdx2-csf": "^0.0.4",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue": "^6.5.16",
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vue/test-utils": "^1.1.0",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.3.0",
    "chromatic": "^5.2.0",
    "eslint": "8",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-storybook": "^0.6.10",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": "^10.4.2",
    "postcss": "^8.4.14",
    "prettier": "^2.1.2",
    "sass": "^1.57.1",
    "shipjs": "0.21.1",
    "vite-svg-loader": "^4.0.0",
    "vue-docgen-web-types": "^0.1.7",
    "vue-intellisense": "^1.0.1",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.10.1",
    "vue-styleguidist": "^4.41.2"
  },
  "browserslist": [
    "last 2 version",
    "> 5% in KR",
    "IE >= 11",
    "iOS >= 11"
  ],
  "lint-staged": {
    "*.{js,vue}": "eslint --ext .js,.vue --ignore-path .gitignore --fix src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "web-types": "dist/web-types.json"
}
